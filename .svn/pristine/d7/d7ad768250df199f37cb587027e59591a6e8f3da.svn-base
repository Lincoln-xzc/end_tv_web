* {
    margin   : 0px;
    padding  : 0px;
    border   : 0px;
    outline  : 0px;
    float    : left;
    position : relative;
    vertical-align : middle;
 -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
         box-sizing: border-box;
}
*:before,*:after {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
html {
    width    : 100%;
	max-width: 100%;
    display  : block;
}
body {
	width      : 100%;
	max-width  : 100%;
    text-align : left;
    font-family: "微软雅黑","sans-serif","Helvetica","Arial","Lucida Grande", "Lucida Sans Unicode", "Verdana"; 
    font-size  : 14px;
	font-weight: normal;
    font-style : normal;
    line-height: 1.5;
	display    : block;
}
h1 { font-size: 36px; }
h2 { font-size: 28px; }
h3 { font-size: 24px; }
h4 { font-size: 18px; }
h5 { font-size: 14px; }
h6 { font-size: 12px; }
h1 {
    padding:20px 0px 20px 0px;
}
h2{
    padding:15px 0px 15px 0px;
}
h3{
    padding:10px 0px 10px 0px;
}
h4,h5,h6 {
    padding:10px 0px 10px 0px;
}
h1,h2,h3,h4,h5,h6 {
    width:100%;
    font-family: inherit;
    font-weight: 700;
    color: inherit;
}
a {
    color      : rgb(0,0,0);
    background : transparent;  /*透明*/
    text-decoration : none;    /*取消下划线 ,如果要设置下划线 ,将值设置为 underline 即可*/
}
a:active,a:hover,a:focus {
  outline: 0;
  text-decoration: none;
}
p {
    width:100%;
    padding-top:5px;
    padding-bottom:5px;
    line-height:25px;
}
small,.small {
	float:none;
    font-size:85%;
}
b,strong {
	float:none;
    font-weight:700;
}
button,select,input[type="button"],input[type="reset"],input[type="submit"] {
  cursor: pointer;
}
button,input,select,textarea {
  font-family: inherit;
  font-size  : inherit;
  line-height: inherit;
}
